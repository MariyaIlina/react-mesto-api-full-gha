    })\n    .then(this._checkResponse);\n  }\n\n  changeLikeCardStatus(_id, isLiked) {\n    return fetch(`${this._baseUrl}/cards/${_id}/likes`, {\n      method: `${isLiked ? \"PUT\" : \"DELETE\"}`,\n      headers: this._headers,\n    }).then(this._checkResponse);\n  }\n  editAvatar = (data) => {\n    return fetch(this._baseUrl + \"/users/me/avatar\", {\n      method: \"PATCH\",\n      headers: this._headers,\n      body: JSON.stringify({\n        avatar: data.avatar,\n      }),\n    }).then(this._checkResponse);\n  };\n}\n\n const api = new Api({\n  baseUrl: \"https://mesto.nomoreparties.co/v1/cohort-59\",\n  headers: {\n    authorization: \"510ed949-2f9c-4a1e-b028-07a35bd485cd\",\n    \"Content-Type\": \"application/json\",\n  },\n});\nexport default api \n","/Users/mariailina/Documents/Praktikum/react-mesto-auth/src/utils/auth.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-auth/src/contexts/CurrentUserContext.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-auth/src/components/Card.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-auth/src/components/PopupWithForm.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/index.js",[],[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/reportWebVitals.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/App.js",["205","206","207","208"],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/Header.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/Main.js",["209","210"],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/Footer.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/Login.js",[],[],"/Users/mariailina/Documents/Praktikum/react-mesto-api-full-gha/frontend/src/components/AddPlacePopup.js",[],[],"/User